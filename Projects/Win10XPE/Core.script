[CheckWaikTool]
Set,%WaikToolOK%,Yes
If,Not,ExistDir,%GTools%\x86,Set,%WaikToolOK%,No
If,Not,ExistDir,%GTools%\x64,Set,%WaikToolOK%,No
If,%WaikToolOK%,Equal,Yes,Begin
  If,Not,ExistFile,%GTools%\x86\BCDBoot\bcdedit.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x64\BCDBoot\bcdedit.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x86\DISM\dism.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x64\DISM\dism.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x86\DISM\imagex.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x64\DISM\imagex.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x86\Oscdimg\oscdimg.exe,Set,%WaikToolOK%,No
  If,Not,ExistFile,%GTools%\x64\Oscdimg\oscdimg.exe,Set,%WaikToolOK%,No
End
If,%WaikToolOK%,Equal,No,Begin
  Set,%answer%,No
  If,Question,"%ProjectName% needs some Windows ADK files (8.50 MB)#$x#$x Download now ?",10,True,Set,%answer%,Yes
  If,%answer%,Equal,Yes,Begin
    Run,%ScriptFile%,GetWaikTool
    Run,%ScriptFile%,CheckWaikTool
  End
  Else,Halt,"You did not download the Windows ADK files required for %ProjectName%."
End

