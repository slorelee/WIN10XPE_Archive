ShellExecute,Open,%GTools%\GWT.exe,"-Win10 -Win10Dism -Silent",%GTools%
//ShellExecute,Open,%GTools%\GWT.exe,"-Win10 -Win10Dism -devcon -Silent",%GTools%
DirMove,%GTools%\ADK_6\x86,%GTools%
If,Not,ExistDir,%GTools%\x64,DirMake,%GTools%\x64
DirMove,%GTools%\ADK_6\amd64\BCDBoot,%GTools%\x64
DirMove,%GTools%\ADK_6\amd64\DISM,%GTools%\x64
DirMove,%GTools%\ADK_6\amd64\Oscdimg,%GTools%\x64
DirDelete,%GTools%\ADK_6

