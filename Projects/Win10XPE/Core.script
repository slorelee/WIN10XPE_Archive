  If,Not,ExistDir,%GTemp%,DirMake,%GTemp%
  ShellExecute,Hide,%GTools%\WimInfo.exe,"-wim:#$q%GTemp%\winre.wim#$q -ini:#$q%GTemp%\WinreInfo.ini#$q"
  If,Not,ExistFile,%GTemp%\WinreInfo.ini,EchoExtended,"Error: Winre.wim Image information missing. A problem occurred to Get Image Information.#$x#$xTry using official MS sources.",Warn,,MessageError,,Halt
  IniRead,%GTemp%\WinreInfo.ini,1,VERSION,%WinreVersion%
  If,Not,%WinreVersion%,Equal,%SourceVersion%,Begin
