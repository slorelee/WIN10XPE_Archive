[LoadRufus]
If,ExistFile,%RufusExe%,Begin
  Message,"To Avoid Any Failure at Startup#$xor USB Key Not Mounted on Drive Y:#$x#$xBe Sure to Use These Recommended Settings :#$x  Partition Scheme : MBR#$x  Target System : BIOS or UEFI#$x  File System : FAT32",Information
  If,ExistFile,%RufusIni%,Begin
    TXTDelLine,%RufusIni%,"EnableWindowsDualUefiBiosMode=1"
    TXTDelLine,%RufusIni%,"EnableWindowsDualUefiBiosMode=0"
  End
  Else,Begin
    FileCreateBlank,%RufusIni%
    TXTAddLine,%RufusIni%,"UpdateCheckInterval=-1",Append
  End
  If,%RufusDualBoot_CheckBox%,Equal,True,TXTAddLine,%RufusIni%,"EnableWindowsDualUefiBiosMode=1",Prepend
  Else,TXTAddLine,%RufusIni%,"EnableWindowsDualUefiBiosMode=0",Prepend
  //-
  If,ExistFile,%ISOFileName%,ShellExecuteEx,Open,%RufusExe%,"-g -f FAT32 -i %ISOFileName%",%GTools%
  Else,ShellExecuteEx,Open,%RufusExe%,"-g -f FAT32",%GTools%
End

