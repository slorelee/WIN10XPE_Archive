[Variables]
%Debug_Display_CheckSysFiles%=False
%ImageName%=%ProjectName%_%Architecture%
%ISOFileName%=%BaseDir%\%ProjectName%_%Architecture%.ISO
%BcdEditExe%=%GTools%\%Architecture%\BCDBoot\bcdedit.exe
%tmout%=5
%DisplayMenu%=False
%Usb7iceExe%=%GTools%\Usb7ice.exe
%RufusExe%=%GTools%\rufus-3.11p.exe
%RufusIni%=%GTools%\rufus.ini
//-
%RegSoftware%=%GTarget_Sys%\config\SOFTWARE
%RegSystem%=%GTarget_Sys%\config\SYSTEM
%RegDefault%=%GTarget_Sys%\config\DEFAULT
%RegDrivers%=%GTarget_Sys%\config\DRIVERS
//-
%KeySoftware%=Tmp_Software
%KeySystem%=Tmp_System
%KeyDefault%=Tmp_Default
%KeyDrivers%=Tmp_Drivers
//-
Copy=Run,%ScriptFile%,PCopy
CopyMui=Run,%ScriptFile%,PCopyMui

