[ShowAdvancedOptions]
System,CURSOR,Wait
Run,%ScriptFile%,AdvancedOptions,True
Visible,%ShowAdvancedOptions_Button%,False,Permanent
Visible,%HideAdvancedOptions_Button%,True,Permanent
System,RefreshInterface
System,CURSOR,Normal

[HideAdvancedOptions]
System,CURSOR,Wait
Run,%ScriptFile%,AdvancedOptions,False
Visible,%ShowAdvancedOptions_Button%,True,Permanent
Visible,%HideAdvancedOptions_Button%,False,Permanent
System,RefreshInterface
System,CURSOR,Normal

[AdvancedOptions]
Visible,%WimOnly_CheckBox%,#1,Permanent
Visible,%NoCDUsbY_CheckBox%,#1,Permanent
Visible,%RufusDualBoot_CheckBox%,#1,Permanent

[PreCheck_Image_Exist]
If,Not,ExistFile,%GSource%\sources\install.wim,EchoExtended,"install.wim image was not found in the Windows 10 source folder.#$x#$xSelect the root of the Windows 10 source folder.",Warn,,Message,,Halt
If,Not,ExistFile,%GSource%\sources\boot.wim,EchoExtended,"boot.wim image was not found in the Windows 10 source folder.#$x#$xSelect the root of the Windows 10 source folder.",Warn,,Message,,Halt

[PreCheck_Hives_Exist]
If,Not,ExistFile,%RegSystem%,EchoExtended,"The System registry hive was not found.#$x#$xThe project must be built beforehand.",Warn,,Message,,Halt
If,Not,ExistFile,%RegSoftware%,EchoExtended,"The Software registry hive was not found.#$x#$xThe project must be built beforehand.",Warn,,Message,,Halt
If,Not,ExistFile,%RegDefault%,EchoExtended,"The Default registry hive was not found.#$x#$xThe project must be built beforehand.",Warn,,Message,,Halt
If,Not,ExistFile,%RegDrivers%,EchoExtended,"The Drivers registry hive was not found.#$x#$xThe project must be built beforehand.",Warn,,Message,,Halt

[process]
Echo,"Processing %ScriptTitle%..."
Run,%ScriptFile%,PreCheck_Image_Exist
Run,%ScriptFile%,PreCheck_Hives_Exist
//-
AddInterface,%ProjectDir%\Core.script,InterfaceBuild,""
StrFormat,SPLIT,%FallBackLanguage%,|,0,%NbLang%
//-
// To be sure, Unload Hive to avoid error 47 (sharing violation eg registry hive loaded) when capturing
Run,%ScriptFile%,HiveUnload,%GTarget%
Run,%ScriptFile%,CleanupFiles
//-
If,%DrvInstPatch_CheckBox%,Equal,True,Run,%ScriptFile%,DrvInstPatch
If,%ControllerPatch_CheckBox%,Equal,True,Run,%ScriptFile%,ControllerPatch
If,Not,ExistFile,%GTarget_Sys%\DeviceSetupManager.dll.org,Run,%ScriptFile%,EjectDevicePatch
//-
Run,%ScriptFile%,Check_Extract_SysFiles
//-
DirDeleteQ,%GISO%\sources
DirMake,%GISO%\sources
//-
Echo,"Packing boot.wim image file, Be patient..."
//ShellExecute,Open,%GTools%\wimlib-imagex.exe,"capture #$q%GTarget%#$q #$q%GISO%\sources\boot.wim#$q #$q%ImageName%#$q #$q%ImageName%#$q --boot --flags=9 --compress=LZX --verbose"
If,%CompressMax_CheckBox%,Equal,True,ShellExecute,%OpenHideOutput%,cmd.exe,"/D /C Echo Packing boot.wim image file, Be patient...&Echo.&wimlib-imagex.exe capture #$q%GTarget%#$q #$q%GISO%\sources\boot.wim#$q #$q%ImageName%#$q #$q%ImageName%#$q --boot --flags=9 --compress=LZX --verbose",%GTools%
Else,ShellExecute,%OpenHideOutput%,cmd.exe,"/D /C Echo Packing boot.wim image file, Be patient...&Echo.&wimlib-imagex.exe capture #$q%GTarget%#$q #$q%GISO%\sources\boot.wim#$q #$q%ImageName%#$q #$q%ImageName%#$q --boot --flags=9 --compress=XPRESS --verbose",%GTools%
If,%ExitCode%,Equal,0,Echo,"Wimlib Packing boot.wim image file return : %ExitCode%"
Else,Begin
  Echo,"Wimlib Packing boot.wim image file return : %ExitCode%",Warn
  EchoExtended,"Error: boot.wim image file was not properly packaged.",Warn,,MessageError,,Halt
End
//-
Run,%ScriptFile%,CreateISO

[DrvInstPatch]
Echo,"Patch DrvInst.exe..."
If,ExistDir,%GTemp%\DrvInstPatch,DirDeleteQ,%GTemp%\DrvInstPatch
DirMake,%GTemp%\DrvInstPatch
ExtractFile,%ScriptFile%,Folder,DrvInstPatch.exe,%GTemp%\DrvInstPatch
//FileCopy,%GTarget_Sys%\drvinst.exe,%GTarget_Sys%\drvinst_org.exe
ShellExecute,Hide,%GTemp%\DrvInstPatch\DrvInstPatch.exe,"P %GTarget_Sys%\drvinst.exe"

[ControllerPatch]
// Patch Windows.UI.CredDialogController.dll to Get the Credentials Window Only If both Remote Desktop and Switch to Admin Enabled
Iniread,%ProjectDir%\Script.Project,Features,Admin,%Admin%
If,%Admin%,Equal,True,Begin
  If,ExistFile,%GTarget_Sys%\mstsc.exe,Begin
    Echo,"Patch Controller.dll to Get the Credentials Window..."
    //FileCopy,%GTarget_Sys%\Windows.UI.CredDialogController.dll,%GTarget_Sys%\Windows.UI.CredDialogController_org.dll
    If,ExistFile,%GTarget_Sys%\Windows.UI.CredDialogController.dll,ShellExecute,Hide,%GTools%\binmay.exe,"-u #$q%GTarget_Sys%\Windows.UI.CredDialogController.dll#$q -s 5C004D0069006E0069004E0054000000 -r 5C004D0069006E0069004E0058000000"
  End
End

[EjectDevicePatch]
ShellExecute,Hide,%GTools%\binmay.exe,"-u #$q%GTarget_Sys%\DeviceSetupManager.dll#$q -s u:SystemSetupInProgress -r u:DisableDeviceSetupMgr"

[Check_Extract_SysFiles]
Echo,"Check and Add System Resources Files..."
If,%Debug_Display_CheckSysFiles%,Equal,True,Set,%Action_After%,Display
Else,Set,%Action_After%,ExtractListFiles
If,%SourceBuild%,Bigger,18300,Set,%CheckmunFiles%,True
Else,Set,%CheckmunFiles%,False
//-
Exec,%ProjectDir%\Features\CheckSysFiles.script,CheckSysFiles,Project,True,%CheckmunFiles%,%Action_After%

[CreateISO]
Echo,"Create ISO"
If,Not,ExistFile,%GISO%\sources\boot.wim,EchoExtended,"boot.wim image was not found in ISO\sources source folder.#$x#$xThe project must be built beforehand.",Warn,,Message,,Halt
StrFormat,SPLIT,%FallBackLanguage%,|,0,%NbLang%
//-
If,%IsoRoot_CheckBox%,Equal,True,Run,%ScriptFile%,IsoRootAdd
//-
// For AIOBoot do not create CDUsb.y if Run All programs from RAM selected
IniRead,%ProjectDir%\script.project,Features,AllInRam,%AllInRam%
If,%NoCDUsbY_CheckBox%-%AllInRam%,Equal,True-True,Begin
  If,ExistFile,%GISO%\CDUsb.y,FileDelete,%GISO%\CDUsb.y
End
Else,Begin
  If,Not,ExistFile,%GISO%\CDUsb.y,Begin
    FileCreateBlank,%GISO%\CDUsb.y
    TXTAddLine,%GISO%\CDUsb.y,"Flag File to mount CD/USB on drive Y:",Append
  End
End
//-
Run,%ScriptFile%,CheckISOBootFiles
If,%ISOBootFilesOK%,Equal,No,Begin
  Run,%ScriptFile%,CheckTargetBootFiles
  If,%TargetBootFilesOK%,Equal,Yes,Run,%ScriptFile%,CopyBootFiles,%GTarget%
  Else,Run,%ScriptFile%,ExtractBootFiles
End
If,%Bcdedit_CheckBox%,Equal,True,Run,%ScriptFile%,SetBcd
//-
FileDeleteQ,%ISOFileName%
If,Not,%WimOnly_CheckBox%,Equal,True,Run,%ScriptFile%,MakeISO

[IsoRootAdd]
Retrieve,FolderSize,%GCustom%\%Architecture%\IsoRoot,%IsoRootSize%
If,%IsoRootSize%,Bigger,0,Begin
  Echo,"Copy Iso Root Files..."
  DirCopy,%GCustom%\%Architecture%\IsoRoot\*,%GISO%
End

[IsoRootOpen]
OpenDir,%GCustom%\%Architecture%\IsoRoot

[CheckISOBootFiles]
Set,%ISOBootFilesOK%,Yes
If,Not,ExistDir,%GISO%\boot,Set,%ISOBootFilesOK%,No
If,Not,ExistDir,%GISO%\efi,Set,%ISOBootFilesOK%,No
If,%ISOBootFilesOK%,Equal,Yes,Begin
  // In case of change of PressAnyKey_CheckBox
  If,%PressAnyKey_CheckBox%,Equal,True,Begin
    If,Not,ExistFile,%GISO%\boot\bootfix.bin,Set,%ISOBootFilesOK%,No
  End
  Else,Begin
    If,ExistFile,%GISO%\boot\bootfix.bin,FileDeleteQ,%GISO%\boot\bootfix.bin
    If,ExistFile,%GISO%\efi\microsoft\boot\efisys.bin,FileDeleteQ,%GISO%\efi\microsoft\boot\efisys.bin
  End
  If,Not,ExistFile,%GISO%\bootmgr,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\bootmgr.efi,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\boot\BCD,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\boot\boot.sdi,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\boot\etfsboot.com,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\efi\microsoft\boot\BCD,Set,%ISOBootFilesOK%,No
  If,Not,ExistFile,%GISO%\efi\microsoft\boot\efisys.bin,Set,%ISOBootFilesOK%,No
  If,%Architecture%,Equal,x64,Begin
    If,Not,ExistFile,%GISO%\efi\boot\bootx64.efi,Set,%ISOBootFilesOK%,No
  End
  Else,Begin
    If,Not,ExistFile,%GISO%\efi\boot\bootia32.efi,Set,%ISOBootFilesOK%,No
  End
End

[CheckTargetBootFiles]
Set,%TargetBootFilesOK%,Yes
If,Not,ExistDir,%GTarget_Win%\Boot,Set,%TargetBootFilesOK%,No
If,%TargetBootFilesOK%,Equal,Yes,Begin
  If,Not,ExistFile,%GTarget_Win%\Boot\PCAT\bootmgr,Set,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\EFI\bootmgr.efi,Set,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\DVD\PCAT\bcd,Set,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\DVD\PCAT\boot.sdi,Set,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\DVD\PCAT\etfsboot.com,Set,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\DVD\EFI\bcd,Set,%TargetBootFilesOK%,No
  //If,Not,ExistFile,%GTarget_Win%\Boot\DVD\EFI\en-US\efisys.bin,%TargetBootFilesOK%,No
  If,Not,ExistFile,%GTarget_Win%\Boot\EFI\bootmgfw.efi,Set,%TargetBootFilesOK%,No
End

[ExtractBootFiles]
Echo,"Extract Bios and EFI boot Files..."
DirDeleteQ,%Gtemp%\ISOBootFiles
DirMake,%Gtemp%\ISOBootFiles
FileDeleteQ,%Gtemp%\ExtractBootFiles
Set,%w%,%Gtemp%\ExtractBootFiles
FileDeleteQ,%w%
FileCreateBlank,%w%
TxtAddline,%w%,\Windows\Boot\PCAT\bootmgr,Append
TxtAddline,%w%,\Windows\Boot\PXE\bootmgr.exe,Append
TxtAddline,%w%,\Windows\Boot\PXE\??-??\bootmgr.exe.mui,Append
TxtAddline,%w%,\Windows\Boot\DVD\PCAT\bcd,Append
TxtAddline,%w%,\Windows\Boot\DVD\PCAT\boot.sdi,Append
TxtAddline,%w%,\Windows\Boot\DVD\PCAT\etfsboot.com,Append
TxtAddline,%w%,\Windows\System32\bootsect.exe,Append
TxtAddline,%w%,\Windows\System32\??-??\bootsect.exe.mui,Append
TxtAddline,%w%,\Windows\Boot\PCAT\memtest.exe,Append
TxtAddline,%w%,\Windows\Boot\PCAT\??-??\memtest.exe.mui,Append
TxtAddline,%w%,\Windows\Boot\Fonts\*.*,Append
//-
TxtAddline,%w%,\Windows\Boot\EFI\bootmgr.efi,Append
TxtAddline,%w%,\Windows\Boot\EFI\??-??\bootmgr.efi.mui,Append
TxtAddline,%w%,\Windows\Boot\EFI\bootmgfw.efi,Append
TxtAddline,%w%,\Windows\Boot\EFI\??-??\bootmgfw.efi.mui,Append
TxtAddline,%w%,\Windows\Boot\DVD\EFI\bcd,Append
TxtAddline,%w%,\Windows\Boot\EFI\memtest.efi,Append
TxtAddline,%w%,\Windows\Boot\EFI\??-??\memtest.efi.mui,Append
//-
If,%PressAnyKey_CheckBox%,Equal,True,Begin
  TxtAddline,%w%,\Windows\Boot\DVD\PCAT\??-??\bootfix.bin,Append
  TxtAddline,%w%,\Windows\Boot\DVD\EFI\??-??\efisys.bin,Append
End
Else,Begin
  TxtAddline,%w%,\Windows\Boot\DVD\EFI\??-??\efisys_noprompt.bin,Append
End
ShellExecute,Hide,%GTools%\wimlib-imagex.exe,"extract #$q%GISO%\sources\boot.wim#$q 1 @#$q%w%#$q --dest-dir=#$q%Gtemp%\ISOBootFiles#$q --no-acls --nullglob"
If,%ExitCode%,Equal,0,Echo,"Wimlib extract Bios and EFI boot Files return: %ExitCode%"
Else,EchoExtended,"Error: Wimlib extract Bios and EFI boot Files return: %ExitCode%",Warn,,MessageError,,Halt
FileDelete,%w%
Run,%ScriptFile%,CopyBootFiles,%Gtemp%\ISOBootFiles

[CopyBootFiles]
Echo,"Copy Bios and EFI boot Files..."
Loop,%ScriptFile%,DirMakeMui_Loop,1,%NbLang%,%GISO%\boot
If,Not,ExistDir,%GISO%\boot\Fonts,DirMake,%GISO%\boot\Fonts
//-
//- Copy(Mui),SourceFolder,File,TargetFolder,[TargetFile]
Copy,#1\Windows\Boot\PCAT,bootmgr,%GISO%,BOOTMGR
CopyMui,#1\Windows\Boot\PXE,bootmgr.exe,%GISO%\boot
If,ExistFile,%GISO%\boot\bootmgr.exe,Begin
  FileCopy,%GISO%\boot\bootmgr.exe,%GISO%\bootmgr.exe
  FileDeleteQ,%GISO%\boot\bootmgr.exe
End
Copy,#1\Windows\Boot\DVD\PCAT,bcd,%GISO%\boot
Copy,#1\Windows\Boot\DVD\PCAT,boot.sdi,%GISO%\boot
Copy,#1\Windows\Boot\DVD\PCAT,etfsboot.com,%GISO%\boot
CopyMui,#1\Windows\System32,bootsect.exe,%GISO%\boot
CopyMui,#1\Windows\Boot\PCAT,memtest.exe,%GISO%\boot
FileCopy,#1\Windows\Boot\Fonts\*.*,%GISO%\boot\Fonts
// Cleanup empty language folder
Loop,%ScriptFile%,CleanupDirMui_Loop,1,%NbLang%,%GISO%\boot
//-----------------------------------------------
// Create EFI folder and copy EFI boot files
//-----------------------------------------------
Loop,%ScriptFile%,DirMakeMui_Loop,1,%NbLang%,%GISO%\efi\boot
Loop,%ScriptFile%,DirMakeMui_Loop,1,%NbLang%,%GISO%\efi\microsoft\boot
If,Not,ExistDir,%GISO%\efi\microsoft\boot\fonts,DirMake,%GISO%\efi\microsoft\boot\fonts
//-
CopyMui,#1\Windows\Boot\EFI,bootmgr.efi,%GISO%\efi\microsoft\boot
If,ExistFile,%GISO%\efi\microsoft\boot\bootmgr.efi,Begin
  FileCopy,%GISO%\efi\microsoft\boot\bootmgr.efi,%GISO%\bootmgr.efi
  FileDeleteQ,%GISO%\efi\microsoft\boot\bootmgr.efi
End
If,%Architecture%,Equal,x64,CopyMui,#1\Windows\Boot\EFI,bootmgfw.efi,%GISO%\efi\boot,bootx64.efi
Else,CopyMui,#1\Windows\Boot\EFI,bootmgfw.efi,%GISO%\efi\boot,bootia32.efi
Copy,#1\Windows\Boot\DVD\EFI,bcd,%GISO%\efi\microsoft\boot
CopyMui,#1\Windows\Boot\EFI,memtest.efi,%GISO%\efi\microsoft\boot
FileCopy,#1\Windows\Boot\Fonts\*.*,%GISO%\efi\microsoft\boot\fonts
// Cleanup empty language folder
Loop,%ScriptFile%,CleanupDirMui_Loop,1,%NbLang%,%GISO%\efi\boot
Loop,%ScriptFile%,CleanupDirMui_Loop,1,%NbLang%,%GISO%\efi\microsoft\boot
//-----------------------------------------------
// Add Press any key to boot...
//-----------------------------------------------
If,%PressAnyKey_CheckBox%,Equal,True,Begin
  Loop,%ScriptFile%,CopyFromMuiDir_Loop,1,%NbLang%,#1\Windows\Boot\DVD\PCAT,bootfix.bin,%GISO%\boot
  If,ExistFile,%GISO%\efi\microsoft\boot\efisys_noprompt.bin,FileDelete,%GISO%\efi\microsoft\boot\efisys_noprompt.bin
  Loop,%ScriptFile%,CopyFromMuiDir_Loop,1,%NbLang%,#1\Windows\Boot\DVD\EFI,efisys.bin,%GISO%\efi\microsoft\boot
End
Else,Begin
  If,ExistFile,%GISO%\boot\bootfix.bin,FileDelete,%GISO%\boot\bootfix.bin
  If,ExistFile,%GISO%\efi\microsoft\boot\efisys.bin,FileDelete,%GISO%\efi\microsoft\boot\efisys.bin
  Loop,%ScriptFile%,CopyFromMuiDir_Loop,1,%NbLang%,#1\Windows\Boot\DVD\EFI,efisys_noprompt.bin,%GISO%\efi\microsoft\boot,efisys.bin
End

[SetBcd]
Echo,"Set boot configuration data localization"
If,Not,ExistDir,%XPECache%,DirMake,%XPECache%
If,ExistFile,%XPECache%\HiveCache.ini,Run,%ScriptFile%,CheckBcdCacheINI
Else,Begin
  If,ExistFile,%XPECache%\bcdboot,FileDeleteQ,%XPECache%\bcdboot
  If,ExistFile,%XPECache%\bcdefi,FileDeleteQ,%XPECache%\bcdefi
End
//-
If,ExistFile,%XPECache%\bcdboot,FileCopy,%XPECache%\bcdboot,%GISO%\Boot\bcd,Nowarn
Else,Run,%ScriptFile%,SetBcdBoot
//-
If,ExistFile,%XPECache%\bcdefi,FileCopy,%XPECache%\bcdefi,%GISO%\efi\microsoft\boot\bcd,Nowarn
Else,Run,%ScriptFile%,SetBcdEfi

[CheckBcdCacheINI]
If,Not,ExistFile,%XPECache%\bcdboot,IniWrite,%XPECache%\HiveCache.ini,BcdCache,bcdboot,False
IniRead,%XPECache%\HiveCache.ini,BcdCache,bcdboot,%bcdbootValue%
If,%bcdbootValue%,Equal,False,Begin
  If,ExistFile,%XPECache%\bcdboot,FileDeleteQ,%XPECache%\bcdboot
End
//-
If,Not,ExistFile,%XPECache%\bcdefi,IniWrite,%XPECache%\HiveCache.ini,BcdCache,bcdefi,False
IniRead,%XPECache%\HiveCache.ini,BcdCache,bcdefi,%bcdefiValue%
If,%bcdefiValue%,Equal,False,Begin
  If,ExistFile,%XPECache%\bcdefi,FileDeleteQ,%XPECache%\bcdefi
End

[SetBcdBoot]
If,%HostOSArch%,Equal,x86,Set,%BcdEditExe%,%GTools%\x86\BCDBoot\bcdedit.exe
If,ExistFile,%BcdEditExe%,Begin
  StrFormat,PATH,%BcdEditExe%,%BcdEditExePath%
  If,ExistFile,%GISO%\Boot\bcd,Begin
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {default} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {default} description #$q%ProjectName% %Architecture%#$q",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {bootmgr} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {memdiag} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {bootmgr} timeout %tmout%",%BcdEditExePath%
    // Display Boot Menu to access Memory Test
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {bootmgr} displaybootmenu %DisplayMenu%",%BcdEditExePath%
    // Show progress bars "windows is loading"
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\Boot\bcd#$q /set {default} bootmenupolicy legacy",%BcdEditExePath%
    If,ExistFile,%GISO%\efi\microsoft\boot\bcd.LOG*,FileDelete,%GISO%\efi\microsoft\boot\bcd.LOG*
    //-
    FileCopy,%GISO%\Boot\bcd,%XPECache%\bcdboot
    IniWrite,%XPECache%\HiveCache.ini,BcdCache,bcdboot,True
  End
End

[SetBcdEfi]
If,%HostOSArch%,Equal,x86,Set,%BcdEditExe%,%GTools%\x86\BCDBoot\bcdedit.exe
If,ExistFile,%BcdEditExe%,Begin
  StrFormat,PATH,%BcdEditExe%,%BcdEditExePath%
  If,ExistFile,%GISO%\efi\microsoft\boot\bcd,Begin
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\boot\bcd#$q /set {default} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\boot\bcd#$q /set {default} description #$q%ProjectName% %Architecture% UEFI#$q",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\boot\bcd#$q /set {bootmgr} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\boot\bcd#$q /set {memdiag} locale %Language%",%BcdEditExePath%
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\Boot\bcd#$q /set {bootmgr} timeout %tmout%",%BcdEditExePath%
    // Display Boot Menu to access Memory Test
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\Boot\bcd#$q /set {bootmgr} displaybootmenu %DisplayMenu%",%BcdEditExePath%
    // Show progress bars "windows is loading"
    ShellExecute,Hide,%BcdEditExe%,"/store #$q%GISO%\efi\microsoft\boot\bcd#$q /set {default} bootmenupolicy legacy",%BcdEditExePath%
    If,ExistFile,%GISO%\efi\microsoft\boot\bcd.LOG*,FileDelete,%GISO%\efi\microsoft\boot\bcd.LOG*
    //-
    FileCopy,%GISO%\efi\microsoft\boot\bcd,%XPECache%\bcdefi
    IniWrite,%XPECache%\HiveCache.ini,BcdCache,bcdefi,True
  End
End

